{
  "name": "@dustinober/glm-monitor",
  "version": "1.0.0",
  "description": "Professional dashboard for monitoring GLM Coding Plan API usage",
  "homepage": "https://github.com/dustinober1/glm-monitor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinober1/glm-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/dustinober1/glm-monitor/issues"
  },
  "type": "module",
  "bin": {
    "glm-monitor": "./bin/glm-monitor.js"
  },
  "files": [
    "bin",
    "dist",
    "scripts",
    "src",
    "README.md"
  ],
  "scripts": {
    "prepare": "npm run build",
    "collect": "node scripts/usage-collector.mjs",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node bin/glm-monitor.js start",
    "monitor": "node bin/glm-monitor.js monitor",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "glm",
    "usage",
    "monitoring",
    "dashboard",
    "cli"
  ],
  "dependencies": {
    "chart.js": "^4.5.1",
    "commander": "^14.0.2",
    "conf": "^15.0.2",
    "express": "^4.22.1",
    "node-notifier": "^10.0.1",
    "opn": "^5.5.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.17",
    "jsdom": "^27.4.0",
    "puppeteer": "^24.34.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
